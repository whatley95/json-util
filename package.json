{
  "name": "json-util",
  "version": "1.0.0",
  "type": "module",
  "description": "A comprehensive Vue.js application for JSON utilities including diff, escape/unescape, beautify/minify, validation, JSONPath and object conversion",
  "main": "index.js",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node start.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "npm run build && gh-pages -d dist",
    "deploy:cf": "set CF_PAGES=1 && npm run build",
    "lint": "eslint --ext .js,.vue,.ts src/",
    "format": "prettier --write \"src/**/*.{js,vue,ts}\""
  },
  "keywords": [
    "json",
    "utilities",
    "vue",
    "diff",
    "beautify",
    "jsonpath",
    "validator",
    "escape",
    "minify"
  ],
  "author": "Whatley (https://whatley.xyz/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/whatley95/json-util.git"
  },
  "homepage": "https://whatley95.github.io/json-util/",
  "dependencies": {
    "diff": "^5.2.0",
    "diff2html": "^3.4.20",
    "json5": "^2.2.3",
    "jsonpath-plus": "^7.2.0",
    "jsonlint-mod": "^1.7.6",
    "pinia": "^2.1.7",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.45.0",
    "eslint-plugin-vue": "^9.17.0",
    "gh-pages": "^6.0.0",
    "prettier": "^3.0.3",
    "terser": "^5.43.1",
    "typescript": "^5.3.3",
    "vite": "^4.5.1",
    "vue-tsc": "^1.8.25"
  }
}